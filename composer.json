    "version": "v1.2.1",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Crastlin",
            "email": "crastlin@163.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "ext-redis": "*"
    },
    "autoload": {
        "psr-4": {
